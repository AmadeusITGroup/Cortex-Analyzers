{
    "name": "AbuseIPDB",
    "version": "1.1",
    "author": "Matteo Lodi; Fabien Bloume, StrangeBee",
    "url": "https://github.com/TheHive-Project/Cortex-Analyzers",
    "license": "AGPL-v3",
    "description": "Checks an IP against AbuseIPDB for abuse score, categories, and recent reports.",
    "dataTypeList": ["ip"],
    "baseConfig": "AbuseIPDB",
    "command": "AbuseIPDB/abuseipdb.py",
    "configurationItems": [
      {
        "name": "key",
        "description": "API key for AbuseIPDB",
        "type": "string",
        "multi": false,
        "required": true
      },
      {
        "name": "days",
        "description": "Check for IP Reports in the last X days",
        "type": "number",
        "multi": false,
        "required": false,
        "defaultValue": 30
      } 
    ],
    "config": {
        "check_tlp": true,
        "max_tlp": 2,
        "auto_extract": false
    },
    "registration_required": true,
    "subscription_required": true,
    "free_subscription": true,
    "service_homepage": "https://www.abuseipdb.com/",
    "service_logo": {
        "path": "assets/abuseipdb.png",
        "caption": "abuseipdb logo"
    },
    "screenshots": [
        {
            "path": "assets/long_report.png",
            "caption": "AbuseIPDB: Long report template"
        }
    ]
}